if (<%= obj_has_errors(@experiment) %> == 1) {
	$('#modal').modal('hide'); 	
	$('#modal').on('hidden.bs.modal', function (e) {
	  <%= return_js_for_edit_modal('experiments',{:experiment => @experiment}).html_safe %>
		$('#modal').modal('show'); 
	})
}
else {
	$('#modal').modal('hide');
	// Replace table with new table
	<%= return_js_for_replacing_collection_partial("#experiments-table-body",@experiments).html_safe %>
}
<%= render 'shared/common_js' %>

